{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-project-vb-de-ingestion-training-1"
		},
		"LSAzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LSAzureSqlDatabase1'"
		},
		"LSAzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-project-db-vb-training.database.windows.net"
		},
		"LSAzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql-project-srv-vb-de-training"
		},
		"LSAzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dev_access_data"
		},
		"LS_AzureBlobStorage_source_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "@{linkedService().storage_arc_loc}"
		},
		"LS_AzureDataLakeStorage1_Raw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().adls_storage_arc_loc}"
		},
		"LS_AzureDataLakeStorage_Raw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().adls_storage_acc_lc}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-project-vb-de-ingestion-training-1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LSAzureSqlDatabase1_password": {
						"value": "[parameters('LSAzureSqlDatabase1_password')]"
					},
					"LSAzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('LSAzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"LSAzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('LSAzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"LSAzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('LSAzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"LS_AzureBlobStorage_source_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('LS_AzureBlobStorage_source_properties_typeProperties_serviceEndpoint')]"
					},
					"LS_AzureDataLakeStorage1_Raw_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage1_Raw_properties_typeProperties_url')]"
					},
					"LS_AzureDataLakeStorage_Raw_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage_Raw_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}